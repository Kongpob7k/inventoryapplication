<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <% brand.forEach(item=>{ %>
        <h2><%= item.brand %></h2>
        <% data.forEach(itr=>{ %>
            <%if(itr.brand === item.brand){ %>
                <li><%=itr.name%></li>
            <%} %>
        <% }) %>
    <%})%>
</body>
</html>